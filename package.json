{
  "name": "dm-script",
  "displayName": "dm-script",
  "description": "DM syntax highlighting support",
  "version": "0.0.2",
  "repository": "https: //github.com/matbryan52/dm-script",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dm",
      "aliases": ["DM Script", "dm"],
      "extensions": [".s"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dm",
      "scopeName": "source.dm",
      "path": "./syntaxes/dm.tmLanguage.json"
    }]
  }
}
